// Experiment with moving pens randomly, and with flocking tendancies to those that come beforepackage{		import com.davidbliss.eb.Canvas;	import flash.display.Sprite;	import flash.events.Event;	import flash.events.KeyboardEvent;		public class Spiral extends Sprite	{		private var canvas:Canvas;		public function Spiral()		{	//lines (test full circum)			canvas = new Canvas(true);						canvas.x=canvas.y=5;			addChild(canvas);						canvas.penUp(true);			canvas.movePen(0, 0);			canvas.penUp(false);			var rotations:int = 35;						//canvas.movePen(2000, 700/rotations);			for (var i:int=0; i<rotations; i++) {				trace(i)				canvas.movePenBy(2000, 700/rotations);			}						/*			// for wyatts			canvas.penUp(true);			canvas.movePenBy(0, -695);			canvas.penUp(false);						for (var i:int=0; i<rotations; i++) {				trace(i)				canvas.movePenBy(2000, 700/rotations);			}			*/						// for cross hatch			for (var i:int=0; i<rotations; i++) {				trace(i)				canvas.movePenBy(2000, -700/rotations);			}						canvas.penUp(true);			//canvas.drawThickCircle(100, 300, 100, 99, 50);		}	}}